<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragmentos.Frag_Gymkhana">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    <TextView
        android:id="@+id/NombreG"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:layout_margin="10dp"
        android:gravity="center"
        android:textSize="20sp" />

        <TextView
            android:id="@+id/LugarG"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/NombreG"
            android:layout_margin="5dp"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:text="Lugar"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/DificultadG"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/LugarG"
            android:layout_margin="5dp"
            android:gravity="center"
            android:text="Dificultad"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/ParticipantesG"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/DificultadG"
            android:layout_margin="5dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:text="Participantes"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/InicioG"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/ParticipantesG"
            android:layout_margin="5dp"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:text="Inicio"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/FinG"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/InicioG"
            android:layout_margin="5dp"
            android:gravity="center"
            android:text="Fin"
            android:textSize="20sp" />

        <FrameLayout
            android:id="@+id/containerPruebas"
            android:layout_below="@+id/FinG"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </RelativeLayout>

</FrameLayout>